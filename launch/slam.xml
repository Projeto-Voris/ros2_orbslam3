<launch>
    <arg name="left_image" default="left/image_raw"/>
    <arg name="right_image" default="right/image_raw"/>


    <node pkg="ros2_orbslam3" exec="stereo" name="stereo" namespace="/sm2/debug"
          args="/ws/src/ros2_orbslam3/vocabulary/ORBvoc.txt
          /ws/src/ros2_orbslam3/config/stereo/Stereo13.yaml
          True" >
        <param name="left_cam" value="$(var left_image)"/>
        <param name="right_cam" value="$(var right_image)"/>
        <param name="namespace" value="debug"/>
<!--        <remap from="/orbslam/pose" to="/debug/pose"/>-->
    </node>
</launch>
